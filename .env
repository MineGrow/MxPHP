[env]
env = develop

[database]
dbtype		= mysqldb
dbprefix	= 
dbname		= mxphp
dbhost		= localhost
username	= mysql_user
password	= mysql_pass
slave		= 0

[database-slave-0]
dbname   	= mxphp

[redis]
host		= localhost
port		= 6379
password	= 

[memcached]
host		= localhost
port		= 11211
password 	= 

[memcache]
host		= localhost
port		= 11211
password 	= 

[mongoDB]
host		= mongodb://localhost
port 		= 27017
username	= mxphp
database 	= mxphp
password	= mxphp

[app_key_map]
315b279901cc47cc21897253b7850d57 = a76adae5c3bbbc3fdf9efb8ca091b7c7

